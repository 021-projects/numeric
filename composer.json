{
    "name": "021/numeric",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "O21\\Numeric\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "O21\\Numeric\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "021-projects",
            "email": "20326979+021-projects@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "laravel/pint": "^1.13"
    },
    "scripts": {
        "test": "phpunit",
        "pint": "pint"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
